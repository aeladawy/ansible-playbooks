---
- name: Retry until a file is available
  hosts: all
  tasks:
    - name: Validate if the file is present
      file:
        path: /etc/rhsm/rhsm.conf
        state: file
      register: lsresult
      until: "lsresult is not failed"
      delay: 2
    - name: Validate if the file is present2
      file:
        path: /etc/rhsm/rhsm.conf
        state: file
      register: lsresult
      until: lsresult is not failed
      delay: 2
    

